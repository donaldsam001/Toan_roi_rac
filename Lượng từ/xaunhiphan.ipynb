    "def xau_nhi_phan(n):\n",
    "    if n==1:\n",
    "        return [[0], [1]]\n",
    "    S=[]\n",
    "    for s in xau_nhi_phan(n-1):\n",
    "        s=[0]+s\n",
    "        S.append(s)\n",
    "    for s in xau_nhi_phan(n-1):\n",
    "        s=[1]+s\n",
    "        S.append(s)\n",
    "        \n",
    "    return S"
   ]
